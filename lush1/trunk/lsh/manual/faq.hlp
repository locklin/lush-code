#? Read lines from a file into a list
.CODE
 (de read-lines(f)
  (reading f
    (let ((ans ()))
      (while (<> (skip-char "\n\r\f") "\e")
        (setq ans (cons (read-string) ans)))
      (reverse ans))))
.P

#? Apply a function to all elements of a vector
interpreted lisp way:
.CODE
 (idx-bloop ((x v)) (func x))
